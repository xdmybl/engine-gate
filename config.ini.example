[kubernetes]
;; kubernetes CRD 的 命名空间
namespace=engine-gate-system
;; kube_config 文件的路径
kube_config=./config
;; kube ApiServer 的静态 token
token=
;; kube ApiServer 的 url
master_url=


[xds]
;; xDS Server 的监听地址, 建议为 20520 端口
listen_ip=0.0.0.0
port=20520

[envoy]
;; 为哪个 envoy 节点提供配置 (通过 node_id 标识 envoy)
node_id=gate-proxy